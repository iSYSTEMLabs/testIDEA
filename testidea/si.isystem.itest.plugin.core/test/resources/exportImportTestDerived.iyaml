env:
  autoConnect: false
  initBeforeRun: false
  downloadOnInit: true
  resetOnInit: false
  runOnInit: true
  stopFunction: main
  defaultRetValName: rv
tests:
- id: 'T1'
  func: [funcTestInt2, [], rv]
  params: [10, 20, 30]
  expect:
  - rv == 61
- id: 'T2'
  locals:
    lvarA: long
    lvarB: long
  init:
    lvarA: 1
    lvarB: 2
  func: [funcTestLong2, [], rv]
  params: [lvarA, lvarB, 40]
  expect:
  - rv == 44
- id: T3
  run: true
  desc: 'only expected section is specified'
  func: [funcTestGlobals]
  expect:
  - rv == 4
- id: 'T4'
  desc: 'intentionally empty test spec'
  func: [funcTestGlobals]
- id: 'T5'
  func: [funcForIntStubTest, [], rv]
  stubs:
  - func:
    - 'stubbedFuncInt'
    - srv
    assign:
      srv: 235
  expect:
  - rv == 234
- id: 'T6'
  init:
    iCounter: 0
  func: [funcTestUInt1, [], rv]
  params: [3]
  expect:
  - rv == 4
  - iCounter == 0
- id: 'full'
  tags:
  - noDerived
  options:
    /IDE/opt1: "ValX"
  locals:
    dblA: double
    dblB: double
  init:
    dblA: 9
    dblB: 8
  func: [funcTestDouble2, [], rv]
  params: [dblA, 3, dblB]
  stubs:
  - func:
    - 'stubbedFunc3'
    - srv
    params:
    - pI
    assign:
      pI: 0
      srv: 0
  expect:
  - rv == dblA + dblB + 3 + 1
